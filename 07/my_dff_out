#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x564faeffa530 .scope module, "test_my_dff" "test_my_dff" 2 1;
 .timescale 0 0;
v0x564faf0455a0_0 .var "clk", 0 0;
v0x564faf045670_0 .var "d", 0 0;
v0x564faf045740_0 .net "q", 0 0, v0x564faf045480_0;  1 drivers
S_0x564faeffa6c0 .scope module, "dff1" "my_dff" 2 6, 3 1 0, S_0x564faeffa530;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "d";
    .port_info 2 /OUTPUT 1 "q";
v0x564faf0348e0_0 .net "clk", 0 0, v0x564faf0455a0_0;  1 drivers
v0x564faf0453c0_0 .net "d", 0 0, v0x564faf045670_0;  1 drivers
v0x564faf045480_0 .var "q", 0 0;
E_0x564faf035170 .event posedge, v0x564faf0348e0_0;
    .scope S_0x564faeffa6c0;
T_0 ;
    %wait E_0x564faf035170;
    %load/vec4 v0x564faf0453c0_0;
    %assign/vec4 v0x564faf045480_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x564faeffa530;
T_1 ;
    %delay 10, 0;
    %load/vec4 v0x564faf0455a0_0;
    %inv;
    %store/vec4 v0x564faf0455a0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x564faeffa530;
T_2 ;
    %vpi_call 2 13 "$monitor", "%t: clk = %b, d = %b, q = %b", $time, v0x564faf0455a0_0, v0x564faf045670_0, v0x564faf045740_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564faf0455a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564faf045670_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564faf045670_0, 0, 1;
    %delay 30, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564faf045670_0, 0, 1;
    %delay 30, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x564faf045670_0, 0, 1;
    %delay 30, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test_my_dff.v";
    "my_dff.v";
